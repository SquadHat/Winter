if game.PlaceId ~= 7551121821 and game.PlaceId ~= 9880062154 then
    return
end

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Title", "DarkTheme")

-- Tabs --

local Main = Window:NewTab("Main")
local Common = Window:NewTab("Common")
local PerkTitles = Window:NewTab("Perk Titles")

-- Local --

local change = workspace.ServerStuff.changeStats

-- Main --

local Main = Main:NewSection("General Titles")

Main:NewButton("Rank + Perk", "None", function()
change:InvokeServer("changetitle", "defaultclass")
game.StarterGui:SetCore("SendNotification", {
Title = "Successfully";
Text = "Changed to Rank&Perk";
Icon = "rbxassetid://6833114846";
Duration = 4;
})
end)

Main:NewButton("Rank", "None", function()
change:InvokeServer("changetitle", "default")
game.StarterGui:SetCore("SendNotification", {
Title = "Successfully";
Text = "Changed to Rank";
